    "format:check": "prettier --check \"**/*.{js,css,html,json,md}\""
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prettier-plugin-css-order": "^2.1.2",
    "stylelint": "^16.26.0",
    "stylelint-config-standard": "^39.0.1",
    "vite": "^5.0.0"
  }
}
